fileName","_jsxFileName","lineNumber","columnNumber","className","src","alt","class","_c","$RefreshReg$"],"sources":["C:/Users/KittyBonny/Documents/GitHub/Diplom/client/src/components/News/News002.jsx"],"sourcesContent":["import React, { useEffect } from 'react';\r\nimport Header from '../Header/Header';\r\nimport Footer from '../Footer/Footer';\r\nimport './NewsComponent.css';\r\nimport banner from '../../img/blogs/002/banner.png';\r\n\r\nconst News002 = () => {\r\n\r\n    useEffect(() => {\r\n        document.title = 'Международный женский день';\r\n      }, []);\r\n\r\n    return (\r\n        <div>\r\n            <Header />\r\n            <main className='main-container'>\r\n            {/* 1 секция баннер */}\r\n            <section className='main-banner'>\r\n                <div className='banner'>\r\n                    <div className='banner-title'>\r\n                        <strong>Международный женский день</strong>\r\n                    </div>\r\n                    <div className='banner-subtitle'>\r\n                        <p>Международный женский день – это годовая традиция, которая призывает к осознанию и привлечению внимания к правам женщин и их социальным, \r\n                экономическим и политическим достижениям</p>\r\n                    </div>\r\n                </div>\r\n                <div className='banner-img'>\r\n                    <img src={banner} alt='' className='banner-img-img'/>\r\n                </div>\r\n            </section>\r\n            {/* 2 секция */}\r\n            <section>\r\n                <div>\r\n                    <p class=\"new_002\">⠀⠀⠀Дорогие женщины! Вся наша команда хочет поздравить вас с Международным женским днём! Желаем вам всегда оставаться \r\n                    такими же сильными, умными, красивыми и душевными! Пусть в вашей жизни будет больше радости, любви, здоровья и благополучия. Пусть каждый день \r\n                    приносит вам новые успехи, достижения и возможности для саморазвития. Спасибо, что вы есть в нашей жизни!\r\n                    </p><br /><br />\r\n                    <p>⠀⠀⠀Этот праздник отмечается 8 марта во многих странах мира. История Международного женского дня началась в начале ХХ века, когда женщины во всем мире начали бороться за свои права на равенство и голосование. Они выступали за свои права \r\n                    на труд и образование, а также протестовали против дискриминации и насилия.\r\n                    </p><br />\r\n                    <p>⠀⠀⠀Сегодня Международный женский день остается важным символом борьбы за равноправие и справедливость во всем мире. Это день, когда мы можем отметить все успехи женщин в борьбе за \r\n                    равенство и продолжить работать над тем, чтобы женщины во всем мире могли жить безопасно, добиваться своих целей и развиваться во всех сферах жизни.\r\n                    </p><br />\r\n                    <p>⠀⠀⠀Международный женский день – это также возможность отметить женщин, которые делают различные вклады в нашу жизнь: матерей, дочерей, сестер, коллег, друзей и прочих. Это день, \r\n                    когда мы можем проявить нашу благодарность и поддержку к женщинам в нашей жизни и в мире в целом.\r\n                    </p><br />\r\n                    <p>⠀⠀⠀Празднование Международного женского дня - это время, чтобы отметить наши ценности и цели, связанные с равенством и добротой. Этот день напоминает нам о том, что мы все можем \r\n                    и должны сделать мир более справедливым и инклюзивным для всех женщин.\r\n                    </p>\r\n                </div>\r\n            </section>\r\n            </main>\r\n            <Footer />\r\n        </div>\r\n    );\r\n};\r\n\r\nexport default News002;"],"mappings":";;AAAA,OAAOA,KAAK,IAAIC,SAAS,QAAQ,OAAO;AACxC,OAAOC,MAAM,MAAM,kBAAkB;AACrC,OAAOC,MAAM,MAAM,kBAAkB;AACrC,OAAO,qBAAqB;AAC5B,OAAOC,MAAM,MAAM,gCAAgC;AAAC,SAAAC,MAAA,IAAAC,OAAA;AAEpD,MAAMC,OAAO,GAAGA,CAAA,KAAM;EAAAC,EAAA;EAElBP,SAAS,CAAC,MAAM;IACZQ,QAAQ,CAA